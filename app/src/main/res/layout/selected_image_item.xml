<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <import type="com.colan.kindercare.R"/>

        <variable
            name="view"
            type="android.view.View"/>

        <variable
            name="imageItem"
            type="com.colan.kindercare.data.model.SelectedImageModel"/>

    </data>
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="wrap_content"
    android:layout_margin="@dimen/_5sdp"
    android:layout_height="wrap_content"
    tools:context=".ui.modules.teacher.dailyActivity.ActivtyChoosingActivity">

    <com.colan.kindercare.ui.cusome_ui.RoundRectCornerImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/test4_img" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_cancel_bg"
        tools:layout_editor_absoluteX="48dp"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintRight_toRightOf="@+id/imageView9"
        tools:layout_editor_absoluteY="4dp" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_cancel"
        app:layout_constraintTop_toTopOf="@+id/imageView10"
        app:layout_constraintRight_toRightOf="@+id/imageView10"
        app:layout_constraintLeft_toLeftOf="@id/imageView10"
        app:layout_constraintBottom_toBottomOf="@+id/imageView10"
        />


</androidx.constraintlayout.widget.ConstraintLayout>
</layout>